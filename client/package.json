{
  "name": "client",
  "version": "1.2.2",
  "description": "React version of Argon Dashboard by Creative Tim",
  "main": "index.js",
  "author": "Creative Tim",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "autoprefixer": "10.4.5",
    "axios": "^1.3.5",
    "bootstrap": "4.6.0",
    "buffer": "^6.0.3",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "concurrently": "^8.0.1",
    "core-js": "^3.31.0",
    "cors": "^2.8.5",
    "http-proxy-middleware": "^2.0.6",
    "moment": "2.29.4",
    "nodemon": "^2.0.22",
    "nouislider": "15.2.0",
    "os-browserify": "^0.3.0",
    "proxy": "http://localhost:5000",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.1.1",
    "react-dom": "18.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "reactstrap": "8.9.0",
    "read-config": "^1.6.0",
    "sass": "1.54.5",
    "styled-components": "^6.0.0-rc.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "express": "^4.18.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "jquery": "3.6.0",
